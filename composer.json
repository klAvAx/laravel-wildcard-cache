{
    "name": "klavax/laravel-wildcard-cache",
    "description": "Cache Wildcard support for Laravel",
    "keywords": ["laravel", "wildcard", "cache"],
    "license": "MIT",
    "homepage": "https://github.com/klAvAx/laravel-wildcard-cache",
    "support": {
      "issues": "https://github.com/klAvAx/laravel-wildcard-cache/issues",
      "source": "https://github.com/klAvAx/laravel-wildcard-cache"
    },
    "authors": [
        {
            "name": "Aldas Å vambaris",
            "email": "aldas.svambaris@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/cache": "^v12",
        "illuminate/database": "^v12",
        "illuminate/session": "^v12"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.10",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^10.5.35|^11.5.3|^12.0.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Cache\\": "src/Cache"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminate\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "check-style": "phpcs",
        "analyse": "phpstan",
        "test": "phpunit --testdox"
    }
}
